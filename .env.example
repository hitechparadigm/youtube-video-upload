# Environment Configuration Example
# Copy this file to .env and fill in your values
# NEVER commit the actual .env file to version control

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your-aws-account-id

# Environment (staging, production)
ENVIRONMENT=staging

# API Configuration (retrieve securely from AWS)
# DO NOT put actual API key here - use AWS CLI to retrieve
API_KEY_ID=your-api-key-id

# Local Development Only
# These should be retrieved from AWS Secrets Manager in production
YOUTUBE_CLIENT_ID=your-youtube-client-id
PEXELS_API_KEY=your-pexels-api-key
PIXABAY_API_KEY=your-pixabay-api-key

# Cost Management
COST_BUDGET_DAILY=50.00
COST_BUDGET_MONTHLY=1000.00

# Logging
LOG_LEVEL=info

# Feature Flags
ENABLE_COST_TRACKING=true
ENABLE_MONITORING=true
ENABLE_DEBUG_MODE=false